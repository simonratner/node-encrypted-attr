{
  "name": "encrypted-attr",
  "version": "1.0.6",
  "description": "Encrypted model attributes in your favourite ORM.",
  "author": "Simon Ratner (https://github.com/simonratner)",
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "attr_encrypted",
    "encrypted",
    "attribute",
    "property",
    "model",
    "orm"
  ],
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "eslint": "^3.19.0",
    "mocha": "^3.5.0",
    "unexpected": "^10.33.2"
  },
  "scripts": {
    "compile": "npm run lint && babel --presets env -d lib/ src/",
    "prepublish": "npm run compile",
    "lint": "eslint . --max-warnings=0",
    "test": "npm run compile && mocha --compilers js:babel-core/register -R spec --recursive"
  },
  "homepage": "https://github.com/simonratner/node-encrypted-attr",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonratner/node-encrypted-attr.git"
  },
  "bugs": "https://github.com/simonratner/node-encrypted-attr/issues",
  "license": "MIT"
}
